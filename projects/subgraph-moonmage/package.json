{
  "name": "subgraph-moonmage",
  "license": "MIT",
  "description": "A subgraph for indexing events emitted by Moonmage.",
  "repository": {
    "type": "git",
    "url": "https://github.com/MoonmageFarms/Moonmage.git",
    "directory": "projects/subgraph-moonmage"
  },
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "test": "graph test",
    "deploy": "graph deploy --node https://api.studio.thegraph.com/deploy/ moonmage",
    "create-local": "graph create --node http://127.0.0.1:8020/ moonmage",
    "deploy-local": "graph deploy --node http://127.0.0.1:8020/ --ipfs http://127.0.0.1:5001 moonmage",
    "create-hosted": "graph create --node http://graph.node.moon.money:8020/ moonmage",
    "remove-hosted": "graph remove --node http://graph.node.moon.money:8020/ moonmage",
    "deploy-hosted": "graph deploy --node http://graph.node.moon.money:8020/ --ipfs http://graph.node.moon.money:5001 moonmage",
    "create-hosted-dev": "graph create --node http://graph.node.moon.money:8020/ moonmage-dev",
    "remove-hosted-dev": "graph remove --node http://graph.node.moon.money:8020/ moonmage-dev",
    "deploy-hosted-dev": "graph deploy --node http://graph.node.moon.money:8020/ --ipfs http://graph.node.moon.money:5001 moonmage-dev",
    "create-hosted-test": "graph create --node http://graph.node.moon.money:8020/ moonmage-testing",
    "remove-hosted-test": "graph remove --node http://graph.node.moon.money:8020/ moonmage-testing",
    "deploy-hosted-test": "graph deploy --node http://graph.node.moon.money:8020/ --ipfs http://graph.node.moon.money:5001 moonmage-testing"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.30.4",
    "@graphprotocol/graph-ts": "^0.27.0",
    "matchstick-as": "^0.5.0"
  },
  "private": true
}
